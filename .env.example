# Example .env file for local development
# Copy this to .env.local and fill in your values

# VAPID Keys for Push Notifications
# Generate with: npx web-push generate-vapid-keys
VITE_VAPID_PUBLIC_KEY=your_public_key_here
VITE_VAPID_PRIVATE_KEY=your_private_key_here
VITE_VAPID_SUBJECT=mailto:example@example.com

# Netlify Configuration
NETLIFY_SITE_ID=your_site_id_here
NETLIFY_BLOBS_TOKEN=your_token_here

# Note: Add these same variables to Netlify Dashboard -> Site settings -> Environment variables
